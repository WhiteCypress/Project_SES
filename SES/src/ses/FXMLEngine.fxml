<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ses.FXMLEngineController">
    <children>
      <ImageView fitHeight="450.0" fitWidth="400.0" layoutX="27.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true" />
      <ComboBox fx:id="materialContList" layoutX="445.0" layoutY="25.0" prefHeight="27.0" prefWidth="281.0" promptText="Material of Container" />
      <Slider fx:id="volContSlider" layoutX="580.0" layoutY="75.0" prefHeight="16.0" prefWidth="146.0" />
      <Label layoutX="445.0" layoutY="75.0" text="Volume of Container" />
      <ComboBox fx:id="materialCombList" layoutX="445.0" layoutY="250.0" prefHeight="27.0" prefWidth="281.0" promptText="Material of Combustion" />
      <TextField fx:id="volLiqText" layoutX="580.0" layoutY="203.0" prefHeight="27.0" prefWidth="146.0" />
      <Label layoutX="445.0" layoutY="208.0" text="Volume of Liquid (L)" />
      <Label layoutX="445.0" layoutY="116.0" text="Thickness of Container" />
      <Slider fx:id="thicContSlider" layoutX="596.0" layoutY="117.0" prefHeight="16.0" prefWidth="126.0" />
      <ComboBox fx:id="typeLiqList" layoutX="445.0" layoutY="156.0" prefHeight="27.0" prefWidth="281.0" promptText="Type of Liquid" />
      <Label layoutX="445.0" layoutY="310.0" text="Vaporization Time:" />
      <Label fx:id="vapTimeLabel" layoutX="571.0" layoutY="310.0" prefHeight="17.0" prefWidth="146.0" />
      <Button fx:id="startEngine" layoutX="449.0" layoutY="399.0" mnemonicParsing="false" onAction="#startEngineButtonAction" prefHeight="27.0" prefWidth="274.0" text="Click to run engine simulation!" />
      <Button fx:id="launchTrain" layoutX="449.0" layoutY="446.0" mnemonicParsing="false" onAction="#launchTrainButtonAction" prefHeight="27.0" prefWidth="274.0" text="Click to launch train simulartion!" />
      <Label layoutX="445.0" layoutY="356.0" text="Engine Power:" />
      <Label fx:id="enginePowerLabel" layoutX="542.0" layoutY="356.0" prefHeight="17.0" prefWidth="146.0" />
    </children>
</AnchorPane>
